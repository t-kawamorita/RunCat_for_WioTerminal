static LGFX lcd;

void setup() {
  lcd.init();
  lcd.setRotation(1);
  lcd.setFont(&fonts::FreeMono9pt7b);
  lcd.fillScreen(lcd.color888(112,112,112));

  pinMode(WIO_5S_LEFT, INPUT_PULLUP);
  pinMode(WIO_5S_RIGHT, INPUT_PULLUP);
}

void loop() {
  int pos = 0;
  int scale = 7, oscale = 7;
  const unsigned char* cats[5] = { cat0, cat1, cat2, cat3, cat4 };
  
  while ( true ) {

    if ( digitalRead(WIO_5S_LEFT) == LOW ) {
      scale = scale > 1 ? scale-1 : scale;
    }
    if ( digitalRead(WIO_5S_RIGHT) == LOW ) {
      scale = scale <= 7 ? scale+1 : scale;
    }

    int w = scale;
    int h = scale;  
    int ofsX = (320 - 32 * w) / 2;
    int ofsY = (240 - 32 * h) / 2;

    const unsigned char *p = cats[pos];
    pos = (pos+1) % 5;
    lcd.startWrite();

    if ( scale != oscale ) {
      lcd.fillScreen(lcd.color888(112,112,112));
      oscale = scale;     
    }
   
    for (int y=0; y<32; y++) {
      for (int x=0; x<32; x++ ) {
        unsigned int pix = (unsigned int)*p++;
        lcd.fillRect( ofsX + x*w, ofsY + y*h, w, h, lcd.color888(pix,pix,pix) );
      }
    }
    lcd.endWrite();
    delay(100);
  }
}
